{
    "manifest_version": 2,
    "name": "Kunji",
    "version": "1.0",
    "description": "A url shortner",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": [
      "activeTab",
      "https://api.dub.co/"
    ]
  }