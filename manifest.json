{
    "manifest_version": 2,
    "name": "Popup Extension",
    "version": "1.0",
    "description": "A simple Chrome extension with a popup",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": [
      "activeTab"
    ]
  }